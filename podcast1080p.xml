---
xmlpage: yes
---
<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:itunes="http://www.itunes.com/dtds/podcast-1.0.dtd" xmlns:atom="http://www.w3.org/2005/Atom" version="2.0">
	<channel>
		<title>Thrideo</title>
		<atom:link href="{{ "/podcast.xml" | prepend: site.url }}" rel="self" type="application/rss+xml" />
		<description>{{ site.description | xml_escape }}</description>
		<link>{{ site.github.url }}/</link>
		<pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
		<lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
		<generator>Jekyll v{{ jekyll.version }}</generator>
		<itunes:new-feed-url>https://ze3kr.tlo.xyz/podcast.xml</itunes:new-feed-url>

		<language>{{ site.language }}</language>
		<copyright>&#xA9;ZE3kr. {{ site.copyright | xml_escape }}</copyright>

		<itunes:subtitle>Thrideo (ZE3kr's Video)</itunes:subtitle>
		<itunes:author>ZE3kr</itunes:author>
		<itunes:summary>{{ site.description | xml_escape }}</itunes:summary>

		<itunes:explicit>{{ site.explicit }}</itunes:explicit>

		<itunes:owner>
			<itunes:name>{{ site.github.owner_name }}</itunes:name>
			<itunes:email>{{ site.email }}</itunes:email>
		</itunes:owner>
		<managingEditor>{{ site.email }} (ZE3kr)</managingEditor>
		<webMaster>{{ site.email }} (ZE3kr)</webMaster>
		<itunes:image href="https:{{ site.urlimg }}/thrideo/itunes/feed.jpg!sq.jpg" />

		<itunes:category text="Sports &amp; Recreation">
			<itunes:category text="Outdoor"/>
			<itunes:category text="Amateur"/>
		</itunes:category>
		<itunes:category text="Technology">
			<itunes:category text="Tech News"/>
		</itunes:category>
		<item>
			<title>此播客已关闭，点击此处获取新播客</title>
			<pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
			<guid isPermaLink="false">donate</guid>
			<itunes:block>yes</itunes:block>
			<enclosure url="https://itunes.apple.com/podcast/id907999392?at=10lJIS" type="text/xml" length="0" />
		</item>
	</channel>
</rss>
